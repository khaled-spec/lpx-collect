    "test:performance": "playwright test tests/performance",
    "test:performance:report": "playwright test tests/performance --reporter=html",
    "test:performance:ci": "playwright test --config playwright.ci.config.ts tests/performance",
    "test:e2e": "playwright test --grep @e2e",
    "test:all:ci": "npm run test:ci && npm run test:visual:ci && npm run test:performance:ci"
